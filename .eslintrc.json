{
    "env": {
        "commonjs": true,
        "node": true
    },

    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-param-reassign": "off",
        "max-len": "warn",
        "max-classes-per-file": "off",
        "no-underscore-dangle": "off",
        "import/no-extraneous-dependencies": "off"
    }
}